ORIGIN  = QCWIN
ORIGIN_VER  = 1.00

PROJ    =GRPICON
DEBUG   =1
PROGTYPE    =1
CALLER  =
ARGS    =
DLLS    =
CVPACK  =1
CC  =cl -c
RC  =rc
CFLAGS_G_WEXE   =/AM /W3 /Ze /D_WINDOWS /DSTRICT /G2w /Zp
CFLAGS_D_WEXE   =/Zi /Od /DDEBUG
CFLAGS_R_WEXE   =/O /DNDEBUG /Ogilt /Gs

CFLAGS  =$(CFLAGS_G_WEXE) $(CFLAGS_R_WEXE)

LFLAGS_G_WEXE   =/A:16/ST:5120
LFLAGS_D_WEXE   =/CO
LFLAGS_R_WEXE   =

LFLAGS  =$(LFLAGS_G_WEXE) $(LFLAGS_R_DEXE)

RCFLAGS =
RESFLAGS    =-t -30
RUNFLAGS    =
H =     APP.H FRAME.H GRPICON.H RESOURCE.H PMGROUP.H ..\ICONCODE\ICONCODE.H PROGMAN.H \
	COMMDLGS.H ..\NWSDLL\ICS.H E:\INCLUDE\COMMDLG.H
RESFILES =  XICON.ICO GRP31.ICO GRP30.ICO GETICON.DLG ..\ICONS\SECUR01A.ICO ..\ICONS\SECUR01B.ICO \
	..\ICONS\COLORS.ICO ..\ICONS\EARTH.ICO ..\ICONS\PAINT_4.ICO WPAPER.DLG
DEFFILE =   GRPICON.DEF
OBJS_EXT =
LIBS_EXT =  ..\NWSDLL\ICS.LIB E:\LIB\COMMDLG.LIB

.rc.res: ; $(RC) $(RCFLAGS) -r $*.rc

all:    $(PROJ).EXE

APP.OBJ:    APP.C $(H)

FRAME.OBJ:  FRAME.C $(H)

GRPICON.RES:    GRPICON.RC $(RESFILES) $(H)

PMGROUP.OBJ:    PMGROUP.C $(H)

PROGMAN.OBJ:    PROGMAN.C $(H)

COMMDLGS.OBJ:   COMMDLGS.C $(H)

$(PROJ).EXE:    APP.OBJ FRAME.OBJ PMGROUP.OBJ PROGMAN.OBJ COMMDLGS.OBJ $(OBJS_EXT) $(DEFFILE)
	echo >NUL @<<$(PROJ).CRF
APP.OBJ +
FRAME.OBJ +
PMGROUP.OBJ +
PROGMAN.OBJ +
COMMDLGS.OBJ +
$(OBJS_EXT)
$(PROJ).EXE

e:\LIB\+
/NOD mlibcew oldnames  libw+
..\NWSDLL\ICS.LIB+
E:\LIB\COMMDLG.LIB
$(DEFFILE);
<<
	link $(LFLAGS) @$(PROJ).CRF
	rc $(RESFLAGS) GRPICON.RES $(PROJ).EXE

$(PROJ).EXE:    GRPICON.RES
	rc $(RESFLAGS) GRPICON.RES $(PROJ).EXE

run: $(PROJ).EXE
	$(PROJ) $(RUNFLAGS)

